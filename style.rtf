body {
    font-family: Arial, sans-serif;
    margin: 0;
    line-height: 1.6;
}

h1, h2 {
    margin-bottom: 20px;
}

header {
    background-color: #343a40;
    color: white;
    padding: 50px 0;
    text-align: center;
}

header a {
    margin: 0 10px;
    text-decoration: none;
}

footer {
    background-color: #343a40;
    color: white;
    padding: 10px 0;
}
